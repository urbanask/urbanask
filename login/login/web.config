<?xml version="1.0"?>
<configuration>

    <system.web>
        <customErrors mode="Off" />
        <compilation debug="true"
            targetFramework="4.0"
            />
        <httpRuntime enableVersionHeader="false" />
        <authentication mode="None" />
    </system.web>
  
    <system.webServer>
        <handlers>
            <add verb="GET,POST"
                path="login"
                name="login"
                type="logins.login"/>
            <add verb="GET,POST"
                path="loginFB"
                name="loginFB"
                type="logins.loginFB"/>
        </handlers>
        <modules>
            <add name="removeHttpHeaders"
                type="removeHttpHeaders.removeHttpHeaders"/>
        </modules>
    </system.webServer>

</configuration>
